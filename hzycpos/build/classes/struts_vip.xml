<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"  
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
 
 <package name="vipManager" extends="json-default" namespace="/vip"> 
		<action name="*_*" class="{1}Action" method="{2}">
			<result name="error" type="json">/error.jsp</result>  
			<result name="login" type="json">/login.jsp</result>  
			<result name="success" type="json">/update.jsp</result>  
			<result name="input" type="json">/update.jsp</result>  
		</action>
	</package> 	
	 
</struts>
