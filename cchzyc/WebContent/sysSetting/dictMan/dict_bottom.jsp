    <script>
	    /*
	    * 删除
	    */
    	function del(id,name){
    		if(confirm("您确认删除该字典项吗?ID："+ id +",名称:"+ name)){
    			//删除
    			htmlobj=$.ajax({url:"<%=basePath%>sysCon/delDictByCode.hzyc?id="+id,async:false});
    			if(htmlobj.responseText == 1){
    				alert("删除成功!");
    			}else{
    				alert("删除失败!");
    			}
    		}
    	}
    	/**
    	* 修改
    	*/
    	var i = 0;
    	var value = "";
    	function update(id,name,code){
    		//将上一次的td变回原始形状
    		/* if(i != 0 ){
    			var oldtd = document.getElementById(i);
        		oldtd.innerHTML = value;
        		
    		}
    		i = id;
    		value = name;
    		
    		//自身样式变为文本框
    		var td = document.getElementById(id);
    		td.innerHTML = "<input id='input"+id+"' value='"+name+"' type='text' /><input type='button' value='确定' onclick='submit("+ code +",input"+id+")' >"; */
    		//window.open("<%=basePath%>stuCon/selOneStudent.hzyc?id="+id,"main");
    		//window.open("<%=basePath%>sysCon/selDictByCode.hzyc?code="+id,"main");
    		var iWidth=620;                          //弹出窗口的宽度; 
       		var iHeight=420; 
            var iTop = (window.screen.availHeight - 30 - iHeight) / 2; 
            //获得窗口的水平位置 
            var iLeft = (window.screen.availWidth - 10 - iWidth) / 2; 
            window.open("<%=basePath%>sysCon/selDictByCode.hzyc?code="+id, "_blank", 'height=' + iHeight + ',innerHeight=' + iHeight + ',width=' + iWidth + ',innerWidth=' + iWidth + ',top=' + iTop + ',left=' + iLeft +
            ',status=no,toolbar=no,menubar=no,location=no,resizable=no,scrollbars=0,titlebar=no'); 	
    	}
    	/*
    	* 详细
    	*/
		function detail(id){
			window.open("<%=basePath%>stuCon/selStudentDetail.hzyc?id="+id,"main");
    	}  
    	
    	//禁用或启用字典项
    	function stop(flag,id){
    		var info = "";
    		var status = 0;
    		//说明本身状态是启用
    		if(flag == 1){
    			info = "确认禁用该选项吗?禁用后可再次启用";
    			//要禁用
    			status = 0;
    		}else{
    			info = "确认启用该选项吗?";
    			status = 1;
    		}
    		if(confirm(info)){
    			//开始
    			htmlobj=$.ajax({url:"../sysCon/disableDict.hzyc?id="+id+"&status="+status,async:false});
    			alert("更新成功...");
    		}
    		
    	}
    	
    	
    	
    	function submit(id,input){
    		alert(input.value);
    		alert($("#"+input).value);
    		//htmlobj=$.ajax({url:"<%=basePath%>stuCon/delStudent.hzyc?id="+id,async:false});
    		
    	}
    
